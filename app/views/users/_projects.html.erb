<div class = "large-4 columns section">
  <% @user_projects.each do |proj| %>
    <div class = "row section">
      <h4><strong><%= proj.project.title %> </strong></h4>

      <% proj.project.images.each do |img| %>
        <% @images = [] %>
        <% @images << img.original %>

      <% end %> 
      <%= image_tag("#{@images.first}.jpg", width: '300px') %>
      <ul class="no-bullet">
        <%=link_to(image_tag("/svg/heart213#{proj.favourited ? "red" : ""}.svg"), favourited_path(proj), method: "post") %>
        <%=link_to(image_tag("/svg/#{proj.completed ? "checkmark4" : "delete38"}.svg"), completed_path(proj), method: "post") %>
      </ul>
    </div>
  <% end %>
</div>


